{
  "name": "vega-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "contracts:compile": "typechain --target ethers-v5 --out-dir src/abis/types './src/abis/**/*.json'",
    "build:staging": "cp env/.env.staging .env && next build",
    "build:production": "cp env/.env.production .env && next build",
    "start": "cp env/.env.local .env && next dev",
    "staging": "cp env/.env.staging .env && next start",
    "production": "cp env/.env.production .env && next start",
    "analyze": "ANALYZE=true next build",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "postinstall": "npm run contracts:compile",
    "prepare": "husky install"
  },
