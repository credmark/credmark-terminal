{
  "name": "vega-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "contracts:compile": "typechain --target ethers-v5 --out-dir src/abis/types './src/abis/**/*.json'",
    "build:staging": "cp env/.env.staging .env && next build",
    "build:production": "cp env/.env.production .env && next build",
    "start": "cp env/.env.local .env && next dev",
    "staging": "cp env/.env.staging .env && next start",
    "production": "cp env/.env.production .env && next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "postinstall": "npm run contracts:compile",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate-sitemap": "npm run build:production && next-sitemap --config next-sitemap.config.js"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.1.0",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@fontsource/roboto": "^4.5.1",
    "@mui/icons-material": "^5.8.2",
    "@mui/material": "^5.8.4",
    "@react-hook/size": "^2.1.2",
    "@reduxjs/toolkit": "^1.8.1",
    "@uniswap/sdk-core": "^3.0.1",
    "@uniswap/v3-sdk": "^3.7.0",
    "@web3-react/core": "^6.1.9",
    "@web3-react/fortmatic-connector": "^6.1.6",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/portis-connector": "^6.1.9",
    "@web3-react/walletconnect-connector": "^6.2.7",
    "@web3-react/walletlink-connector": "^6.2.7",
    "axios": "^0.24.0",
    "chakra-react-select": "^4.0.0",
    "color": "^4.1.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "dequal": "^2.0.2",
    "echarts": "^5.3.3",
    "echarts-for-react": "^3.0.2",
    "ethers": "^5.5.1",
    "focus-visible": "^5.2.0",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "fuse.js": "^6.6.2",
    "jsbi": "^3.2.5",
    "lodash.flatmap": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "luxon": "^2.4.0",
    "mathjs": "^10.4.3",
    "next": "^12.1.6",
    "next-seo": "^5.4.0",
    "nprogress": "^0.2.0",
    "react": "^18.1.0",
    "react-csv": "^2.2.2",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.1.0",
    "react-highlight-words": "^0.18.0",
    "react-json-tree": "^0.17.0",
    "react-redux": "^8.0.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@chakra-ui/storybook-addon": "^3.0.0",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@next/bundle-analyzer": "^12.1.6",
    "@next/eslint-plugin-next": "^12.1.6",
    "@storybook/addon-actions": "^6.5.4",
    "@storybook/addon-essentials": "^6.5.4",
    "@storybook/addon-interactions": "^6.5.4",
    "@storybook/addon-links": "^6.5.4",
    "@storybook/builder-webpack5": "^6.5.4",
    "@storybook/manager-webpack5": "^6.5.4",
    "@storybook/react": "^6.5.4",
    "@storybook/testing-library": "^0.0.11",
    "@typechain/ethers-v5": "^8.0.2",
    "@types/color": "^3.0.2",
    "@types/gtag.js": "^0.0.8",
    "@types/lodash.flatmap": "^4.5.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/luxon": "^2.3.2",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.9",
    "@types/react-csv": "^1.1.2",
    "@types/react-dom": "^18.0.4",
    "@types/react-highlight-words": "^0.16.4",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "babel-loader": "^8.2.5",
    "core-js": "^3.22.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
