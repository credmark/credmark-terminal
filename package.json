    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-storybook": "^0.5.11",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "next-sitemap": "^2.5.25",
    "next-transpile-modules": "^9.0.0",
    "prettier": "^2.4.1",
    "typechain": "^6.0.2",
    "typescript": "4.4.4",
    "webpack": "^5.72.1"
  },
  "overrides": {
    "react-element-to-jsx-string": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "@mdx-js/react": {
      "react": "$react"
    },
    "react-inspector": {
      "react": "$react"
    }
  }
}
